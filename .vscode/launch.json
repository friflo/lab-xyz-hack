	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "read sample",
			"program": "${workspaceFolder}\\read-json.js",
			"args": [
				"--in", 	"source/sample.json",
				"--out", 	"out/sample.json",
				"--prefix",	"test",
				"--pretty"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "read bigger sample",
			"program": "${workspaceFolder}\\read-json.js",
			"args": [
				"--in", 	"source/states.json",
				"--out", 	"out/states.json",
				"--prefix",	"test",
				"--pretty",
				"--max",    "500"
			]
		}
	]
}